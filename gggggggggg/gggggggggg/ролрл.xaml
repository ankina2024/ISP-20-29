<Window x:Class="gggggggggg.ролрл"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:gggggggggg"
        mc:Ignorable="d"
        Title="ролрл" Height="300" Width="600">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*"/>
            <ColumnDefinition Width="2*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="Load" Grid.Column="1" Content="Load&#xD;&#xA;" HorizontalAlignment="Left" Margin="38,42,0,0" VerticalAlignment="Top" Height="64" Width="94" RenderTransformOrigin="0.5,0.5" Click="Load_Click">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="0.322"/>
                    <RotateTransform Angle="-1.081"/>
                    <TranslateTransform X="0.18" Y="-0.003"/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <StackPanel Grid.Column="0" x:Name="Clients"/>

    </Grid>
</Window>
